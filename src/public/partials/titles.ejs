<div class="line sponsor">
    <div class="header">CHAPTERS</div>
    
    <% if (data.stream && data.stream.titleHistory && data.stream.titleHistory.length > 0) { %>
        <% data.stream.titleHistory.forEach((title, index) => { %>
            <div class="shiftleft midtext w-50 veryCloseLineHeight mb-4"><%= title %></div>
        <% }); %>
    <% } %>
    
    <% if (data.stream && data.stream.startTime) { %>
        <div class="shiftleft midtext ">
            <%= new Date(data.stream.startTime).toLocaleDateString('en-US', { 
                year: 'numeric', 
                month: 'long', 
                day: 'numeric' 
            }) %>
        </div>
    <% } %>

</div>